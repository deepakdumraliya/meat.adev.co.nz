<!--=================================
        Meet our meat -->
<section class="space-ptb bg-light">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-lg-6">
				<div class="section-title">
                <h2>{{section.category_Title}}</h2>
				{{section.category_Desc|raw}}
				</div>
			</div>
		</div>
	</div>
	<div class="gallery mt-5">
		<div class="container">
			<div class="row">
            {% for navItem in section.catNavItems %}


				<div class="col-xs-12 col-lg-3 col-md-4 col-sm-6 mt-4">
					<div class="img-container">
						<img class="img-fluid" src="{{ navItem.image.getLink() }}"/>
						<div class="img-content">
							<h4 class="category">
								<a href="{{ navItem.getNavPath() }}">{{ navItem.getNavLabel()|raw }}</a>
							</h4>
						</div>
					</div>
				</div>
                {% endfor %}
			</div>
		</div>
	</div>
</section>



