{% extends 'pages/page.twig' %}

{# content_wrapper is the block most likely to be overidden (to include multiple content sections), 
 # but on some sites it may only be necessary to override the content block, or not at all #}
{% block content_wrapper %}
{# {{dump(getallfeatured)}} #}



	{{ parent() }}
	{%	if page.display_Category	%} 


		{% include 'pages/front-sections/categoryblock.twig' with {
			'section':{
				'displayCategory':page.display_Category,
				'category_Title':page.category_Title,
				'category_Desc':page.category_Desc,
				'catNavItems' : catNavItems
			}
		} only %}
	{% endif %}
	{% include 'pages/front-sections/premiummeat.twig' with {
			'section':{
				'premiumMeatContent':page.premiumMeatContent,
				'meatImage':page.meatImage
			}
		} only
	%}
	{% include 'products/sections/featured.twig' %}
	{% include 'pages/front-sections/testimonialsection.twig'  with {
			'section':{
				'testimonialContent':page.testimonialContent,
				'testimonialImage':page.testimonialImage
			}
	} only %}





{% endblock %}
